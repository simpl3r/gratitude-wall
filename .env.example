# OnchainKit API Key (получить на https://portal.cdp.coinbase.com/)
NEXT_PUBLIC_ONCHAINKIT_API_KEY=your_onchainkit_api_key_here

# WalletConnect Project ID (получить на https://cloud.walletconnect.com/)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id_here

# Base RPC URL (опционально, по умолчанию используется публичный RPC)
NEXT_PUBLIC_BASE_RPC_URL=https://mainnet.base.org

# Контракт для чекинов (уже настроен)
NEXT_PUBLIC_CONTRACT_ADDRESS=0x61026a5CF6F7F83cc6C622B1bBA7B3a4827b8026

# Farcaster App Configuration
NEXT_PUBLIC_FARCASTER_APP_NAME=Gratitude Wall
